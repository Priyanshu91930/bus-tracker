{
  "expo": {
    "name": "bus_tracker",
    "slug": "bus_tracker",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/clg.png",
    "scheme": "bustracker",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/clg.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.bus_tracker",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": {
            "scheme": "com.anonymous.bus_tracker",
            "host": "oauth2redirect"
          },
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "config": {
        "googleSignIn": {
          "certificateHash": "YOUR_CERTIFICATE_HASH"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/clg.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/clg.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
